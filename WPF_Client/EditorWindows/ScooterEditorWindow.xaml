<Window x:Class="C6XSDH_HFT_2021222.WPFClient.EditorWindows.ScooterEditorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:C6XSDH_HFT_2021222.WPFClient.EditorWindows"
        mc:Ignorable="d"
        Title="ScooterEditorWindow" Height="450" Width="300">
    <StackPanel x:Name="stackpanel">
        <Label Content="Model" Background="Bisque"/>
        <TextBox Text="{Binding Model}"/>
        <Label Content="Brand" Background="Bisque"/>
        <TextBox Text="{Binding Brand}"/>
        <Label Content="Price" Background="Bisque"/>
        <TextBox Text="{Binding Price}"/>
        <Label Content="Rating" Background="Bisque"/>
        <TextBox Text="{Binding Rating}"/>
        <Label Content="Speed" Background="Bisque"/>
        <TextBox Text="{Binding Speed}"/>
        <Label Content="Range" Background="Bisque"/>
        <TextBox Text="{Binding Range}"/>
        <Button Content="Save" Click="Button_Click"/>
    </StackPanel>
</Window>
