<Window x:Class="C6XSDH_HFT_2021222.WPFClient.EditorWindows.BrandEditorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:C6XSDH_HFT_2021222.WPFClient.EditorWindows"
        mc:Ignorable="d"
        Title="BrandEditorWindow" Height="200" Width="300">
    <StackPanel x:Name="stackpanel">
        <Label Content="Brand name" Background="Bisque"/>
        <TextBox Text="{Binding BrandName}"/>
        <Button Content="Save" Click="Button_Click"/>
    </StackPanel>
</Window>
